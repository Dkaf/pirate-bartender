  "bugs": {
    "url": "https://github.com/Dkaf/pirate-bartender/issues"
  },
  "homepage": "https://github.com/Dkaf/pirate-bartender#readme",
  "dependencies": {
    "jquery": "^3.1.0"
  },
  "devDependencies": {
    "webpack": "^1.13.2"
  }
}
